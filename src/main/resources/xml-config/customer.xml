<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="customer1" class="platform.codingnomads.co.corespring.lab.Customer">
        <property name="firstName" value="Bill"/>
        <property name="lastName" value="Bubbins"/>
    </bean>

    <bean id="customer2" class="platform.codingnomads.co.corespring.lab.Customer">
        <property name="firstName" value="Stank"/>
        <property name="lastName" value="Johnson"/>
    </bean>
</beans>